{
  "name": "@dnd-assistant/renderer",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_DEV_SERVER_URL=http://127.0.0.1:5173 npm run dev:start",
    "dev:start": "node -e \"console.log('[renderer] starting Vite at ' + process.env.VITE_DEV_SERVER_URL)\" && cross-env VITE_DEV_SERVER_URL=http://127.0.0.1:5173 vite --host 127.0.0.1 --port 5173 --strictPort",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  }
}